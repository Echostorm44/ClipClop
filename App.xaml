             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ClipClop" Startup="Application_Startup"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="FontAwesomeSolid">/ClipClop;component/Fonts/FontAwesomeSolid.otf#Font Awesome 6 Free Solid</FontFamily>
        <FontFamily x:Key="FontAwesomeReg">/ClipClop;component/Fonts/FontAwesomeReg.otf#Font Awesome 6 Free Regular</FontFamily>
        
        
    </Application.Resources>
</Application>
